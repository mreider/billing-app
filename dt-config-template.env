# Dynatrace Configuration Template
# Copy this file to dt-config.env and fill in your actual values

# Dynatrace OpenTelemetry endpoint
DT_OTEL_ENDPOINT=https://abc12345.live.dynatrace.com/api/v2/otlp

# Dynatrace API token with appropriate permissions
DT_API_TOKEN=dt0c01.EXAMPLE1234567890ABCDEFGHIJKLMNOPQRSTUVWXYZ

# Dynatrace environment ID for Lambda layer
DT_TENANT_ID=abc12345

# Dynatrace environment URL
DT_TENANT_URL=https://abc12345.live.dynatrace.com

# Dynatrace connection parameters for Lambda layer
DT_CONNECTION_AUTH=Api-Token dt0c01.EXAMPLE1234567890ABCDEFGHIJKLMNOPQRSTUVWXYZ
DT_CONNECTION_BASE_URL=https://abc12345.live.dynatrace.com
